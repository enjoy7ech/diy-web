{
    "presets": [
        "@babel/preset-env",
        "@vue/babel-preset-jsx"
    ],
    "plugins": [
        "dynamic-import-node",
        [
            "@babel/plugin-transform-modules-commonjs",
            {
                "allowTopLevelThis": true,
                "loose": true
            }
        ],
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-function-bind",
        "@babel/plugin-transform-runtime",
    ]
}